<Window x:Class="Trafostanice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trafostanice"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" MinHeight="350" MinWidth="525">
    <Grid x:Name="MainViewGrid">
        <Menu VerticalAlignment="Top" Height="30">
            <MenuItem Height="30" Width="105" Header="Pretraga" FontSize="16" >
                <MenuItem x:Name="gradoviMenuItem" Header="Gradovi" Click="gradoviMenuItem_Click" />
                <MenuItem x:Name="trafostaniceMenuItem" Header="Trafistanice" Click="trafostaniceMenuItem_Click" />
            </MenuItem>
        </Menu>
        <Frame x:Name="MainFrame" Margin="0,30,0,0" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
